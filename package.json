{
  "name": "samplescope",
  "productName": "Samplescope",
  "version": "0.10.0",
  "description": "Search samples over internet sources",
  "main": ".webpack/main",
  "repository": "https://github.com/jamland/samplescope",
  "scripts": {
    "start": "DEV_MODE=true electron-forge start",
    "start:watch": "DEV_MODE=true nodemon --exec 'electron-forge start'",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "lint:app": "electron-forge lint"
  },
  "keywords": [
    "audio",
    "samples",
    "audio samples",
    "mp3",
    "wav",
    "ogg",
    "music production"
  ],
  "author": {
    "name": "Andy Burkovetsky",
    "email": "jamandyjam@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "appBundleId": "com.github.jamland.samplescope",
        "icon": "./assets/icons/icon.icns",
        "appCategoryType": "public.app-category.music"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "samplescope",
            "certificateFile": "./private/samplescope-dev.p12",
            "certificatePassword": "",
            "iconUrl": "./assets/icons/icon.ico",
            "loadingGif": ""
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@bugsnag/js": "^7.3.0",
    "@bugsnag/plugin-react": "^7.3.0",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/wavesurfer.js": "^3.3.1",
    "abortcontroller-polyfill": "^1.5.0",
    "browser-request": "^0.3.3",
    "electron-dl": "^1.14.0",
    "electron-open-link-in-browser": "^1.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-state": "^5.0.3",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "milligram": "^1.3.0",
    "mini.css": "^3.0.1",
    "node-http-xhr": "^1.3.4",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^2.3.1",
    "react-feather": "^2.0.8",
    "react-infinite-scroller": "^1.2.4",
    "react-query-devtools": "^2.4.2",
    "react-transition-group": "^4.4.1",
    "react-use": "^13.27.1",
    "styled-components": "^4.4.1",
    "wavesurfer.js": "^4.0.1",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@electron-forge/cli": "6.0.0-beta.45",
    "@electron-forge/maker-deb": "6.0.0-beta.45",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "6.0.0-beta.45",
    "@electron-forge/maker-zip": "6.0.0-beta.45",
    "@electron-forge/plugin-webpack": "6.0.0-beta.45",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "devtron": "^1.4.0",
    "dotenv-webpack": "^1.8.0",
    "electron": "^7.2.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-standard": "^4.0.1",
    "node-loader": "^0.6.0",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.21",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0"
  }
}
