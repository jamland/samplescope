{
  "name": "samplescope",
  "productName": "Samplescope",
  "version": "1.0.0",
  "description": "Search samples over internet sources",
  "main": ".webpack/main",
  "repository": "https://github.com/jamland/samplescope",
  "scripts": {
    "start": "DEV_MODE=true electron-forge start",
    "start:watch": "DEV_MODE=true nodemon --exec 'electron-forge start'",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "lint:app": "electron-forge lint",
    "test": "echo \"No tests configured\""
  },
  "keywords": [
    "audio",
    "samples",
    "audio samples",
    "mp3",
    "wav",
    "ogg",
    "music production"
  ],
  "author": {
    "name": "Andy Burkovetsky",
    "email": "jamandyjam@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "appBundleId": "com.github.jamland.samplescope",
        "appCopyright": "MIT License",
        "icon": "./assets/icons/icon.icns",
        "appCategoryType": "public.app-category.music",
        "osxSign": {
          "identity": "Developer ID Application: Andrii Pylypchuk / Jamland (LT94ZKYDCJ)",
          "hardened-runtime": true,
          "entitlements": "entitlements.plist",
          "entitlements-inherit": "entitlements.plist",
          "signature-flags": "library"
        },
        "osxNotarize": {
          "appleId": "desireddada@gmail.com",
          "appleIdPassword": ""
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "samplescope",
            "certificateFile": "./private/samplescope-dev.p12",
            "certificatePassword": "",
            "iconUrl": "./assets/icons/icon.ico",
            "loadingGif": ""
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@bugsnag/js": "^7.3.4",
    "@bugsnag/plugin-react": "^7.3.4",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/wavesurfer.js": "^3.3.1",
    "abortcontroller-polyfill": "^1.5.0",
    "browser-request": "^0.3.3",
    "electron-dl": "^3.0.2",
    "electron-open-link-in-browser": "^1.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-state": "^5.0.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "milligram": "^1.4.1",
    "node-http-xhr": "^1.3.4",
    "node-localstorage": "^2.1.6",
    "normalize.css": "^8.0.1",
    "nucleus-nodejs": "^3.0.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^2.3.2",
    "react-feather": "^2.0.8",
    "react-infinite-scroller": "^1.2.4",
    "react-query-devtools": "^2.4.7",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.3.4",
    "styled-components": "^5.2.0",
    "universal-analytics": "^0.4.23",
    "wavesurfer.js": "^4.0.1",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@electron-forge/cli": "6.0.0-beta.53",
    "@electron-forge/maker-deb": "6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "6.0.0-beta.53",
    "@electron-forge/maker-zip": "6.0.0-beta.53",
    "@electron-forge/plugin-webpack": "6.0.0-beta.53",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^2.0.0",
    "electron": "^10.1.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.1.0",
    "node-loader": "^1.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "react-hot-loader": "^4.12.21",
    "style-loader": "^1.2.1",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0"
  }
}
